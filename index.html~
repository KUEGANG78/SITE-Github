	( function() {
		function init() {
			var bar = document.querySelector( '#wpadminbar' );
			if ( ! bar ) {
				return;
			}
			bar.addEventListener( 'click', function( event ) {
				var img = '//pixel.wp.com/g.gif?v=wpcom-no-pv&x_adminbar=',
					clicked = false,
					pageLocation;

				var target = event.target;

				if ( target.getAttribute( 'id' ) ) {
					clicked = target.getAttribute( 'id' );
				} else if ( target.parent && target.parent.getAttribute( 'id' ) ) {
					clicked = target.parent.getAttribute( 'id' );
				} else if ( target.offsetParent && target.offsetParent.getAttribute( 'id' ) ) {
					clicked = target.offsetParent.getAttribute( 'id' );
				}
				if ( 'string' === typeof clicked ) {
					if ( clicked.indexOf( 'wp-admin-bar-blog-' ) > -1 ) {
						var suffix = clicked.slice( -2 ),
							valid = [ '-d', '-n', '-s', '-c', '-v' ];
						if ( 'wp-admin-bar-blog-stats' !== clicked ) {
							clicked = 'wp-admin-bar-my-sites';
							if ( -1 !== valid.indexOf( suffix ) ) {
								clicked += suffix;
							}
						}
					} else if ( clicked.indexOf( 'wp-admin-bar-theme-' ) > -1 ) {
						clicked = 'wp-admin-bar-theme';
					} else if ( clicked.indexOf( 'shortlink' ) > -1 ) {
						clicked = 'wp-admin-bar-shortlink';
					}
				}

				if ( 'string' == typeof clicked && -1 !== [
					// Blog
					'wp-admin-bar-blog',
					'wp-admin-bar-dashboard-top',
					'wp-admin-bar-blog-stats',
					'wp-admin-bar-comments',
